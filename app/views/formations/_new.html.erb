<% formation = Formation.new %>
<%= formation.inspect %>
<% form_for :formation, formation, :url => { :action => "create", :id => formation.id } do |f| %>
  <%= f.error_messages %>
 <tr>
<td><%= f.text_field :ldap_desc%></td>
<td><%= f.text_field :desc %></td>
<td><%= f.submit 'Create' %></td>
  </tr>
<% end %>
